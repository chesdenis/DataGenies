<div class="card-body">
    <div class="top-row px-4">
        <input type="text" class="form-control" placeholder="Type to search">
    </div>
    <h3>RunTime instance properties</h3>
    @if (Properties.Count == 0)
    {
        <p>Instanse is not running</p>
    }
    <table class="table">
        <tr>
            <th> Type name: </th>
            <th> Property Name: </th>
            <th> Value: </th>
            <th> </th>
            <th> </th>
        </tr>


        @foreach (var property in this.Properties)

        {
            <tr>
                <td>@PropertyTypeNameWithMargin(property.NestingLevel, property.PropertyTypeName)</td>
                <td>@property.PropertyName</td>
                <td>@property.ProperyValue</td>
            </tr>
        }

    </table>
</div>